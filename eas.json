{
  "cli": {
    "version": ">= 7.6.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "medium",
        "autoIncrement": true
      },
      
      "android": {
        "resourceClass": "medium",
        "autoIncrement": true
      },
      "env": {
        "APP_ENV": "development",
        "API_URL": "https://dev.api.example.com"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "medium",
        "autoIncrement": true
      },
      "android": {
        "resourceClass": "medium",
        "autoIncrement": true
      },
      "channel": "preview",
      "env": {
        "APP_ENV": "preview",
        "API_URL": "https://preview.api.example.com"
      }
    },
    "production": {
      "distribution": "store",
      "ios": {
        "resourceClass": "medium",
        "autoIncrement": true
        // REMOVED: "production": true, // This is not allowed here. The profile name 'production' implies it.
      },
      "android": {
        "resourceClass": "medium",
        "autoIncrement": true
      },
      "channel": "production",
      "env": {
        "APP_ENV": "production",
        "API_URL": "https://api.example.com"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "your-apple-id@example.com",
        "ascAppId": "your-app-store-connect-app-id"
        // REMOVED: "teamId": "YOUR_APPLE_DEVELOPER_TEAM_ID", // This is handled by EAS credentials, not here.
        // As discussed before, other submission properties like apiKeyPath, fastlane, track
        // are generally arguments to 'eas submit' or configured via 'eas credentials'.
      },
      "android": {
        "serviceAccountKeyPath": "./private_keys/google-play-service-account.json"
      }
    },
    "preview": {
      "ios": {
        "appleId": "your-apple-id@example.com"
      },
      "android": {
        "serviceAccountKeyPath": "./private_keys/google-play-service-account.json"
      }
    }
  }
}